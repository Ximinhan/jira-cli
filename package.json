{
  "name": "jira-cli",
  "description": "A JIRA Command Line Interface",
  "version": "1.5.3",
  "author": "Dan Shumaker <danpshumaker@gmail.com> (https://danshumaker.github.io/)",
  "contributors": [],
  "scripts": {
    "test": "ava",
    "test:e2e": "bin/tests.sh && exit 1",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "start": "npm run global",
    "dev": "node bin/jira.js",
    "global": "npm i -g && jira",
    "clean": "rimraf -rf ./.jira-cli",
    "clean:global": "rm -rf ~/.jira-cli"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "alasql": "^0.4.11",
    "async": "^2.6.3",
    "cheerio": "^1.0.0-rc.3",
    "cli-table": "^0.2.0",
    "commander": "~1.1.1",
    "dargs": "^5.1.0",
    "fs": "0.0.1-security",
    "gulp-babel": "^8.0.0",
    "moment": "^2.24.0",
    "nodemailer": "^6.4.6",
    "openurl": "^1.1.0",
    "qs": "~> 1.0.0",
    "requirejs": "~2.1.5",
    "superagent": "~3.7.0",
    "underscore": "^1.10.2"
  },
  "engines": {
    "node": ">= 0.8"
  },
  "keywords": [
    "jira",
    "api",
    "client",
    "command",
    "cli",
    "tool",
    "command-line-junky"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/danshumaker/jira-cli.git"
  },
  "bin": {
    "jira": "./bin/jira.js"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-amd": "^7.9.0",
    "ava": "^3.7.0",
    "babel-plugin-transform-amd-to-commonjs": "^1.4.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "gulp": "^4.0.2",
    "jslint": "^0.12.1",
    "rimraf": "^3.0.2",
    "testr": "0.0.5"
  }
}
